{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "0697aa92-cbff-492a-ae84-d1d9b0fd0029"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "my-lib": {
      "projectType": "library",
      "root": "projects/my-lib",
      "sourceRoot": "projects/my-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/my-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/my-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/my-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/my-lib/src/test.ts",
            "tsConfig": "projects/my-lib/tsconfig.spec.json",
            "karmaConfig": "projects/my-lib/karma.conf.js"
          }
        }
      }
    },
    "angular-common-component": {
      "projectType": "library",
      "root": "projects/angular-common-component",
      "sourceRoot": "projects/angular-common-component/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/angular-common-component/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-common-component/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/angular-common-component/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular-common-component/src/test.ts",
            "tsConfig": "projects/angular-common-component/tsconfig.spec.json",
            "karmaConfig": "projects/angular-common-component/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "my-lib"
}
